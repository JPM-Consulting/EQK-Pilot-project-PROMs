definition
	OBSERVATION[at0000] matches {    -- Knee Injury and Osteoarthritis Outcome Score - Short Form (KOOS-12)
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									CLUSTER[at0161] occurrences matches {0..*} matches {*}    -- General instructions
									CLUSTER[at0004] occurrences matches {0..1} matches {    -- Pain
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0005] occurrences matches {0..1} matches {    -- 1. Knee pain frequency
												value matches {
													0|[local::at0006],
													1|[local::at0007],
													2|[local::at0008],
													3|[local::at0009],
													4|[local::at0010]
												}
											}
											ELEMENT[at0164] occurrences matches {0..1} matches {    -- Instruction
												value matches {
													DV_CODED_TEXT matches {
														defining_code matches {
															[local::at0165;at0165]    -- What amount of knee pain have you experienced the last week during the following activities?
														}
													}
												}
											}
											ELEMENT[at0017] occurrences matches {0..1} matches {    -- 2. Walking on a flat surface
												value matches {
													0|[local::at0018],
													1|[local::at0019],
													2|[local::at0020],
													3|[local::at0021],
													4|[local::at0022]
												}
											}
											ELEMENT[at0023] occurrences matches {0..1} matches {    -- 3. Going up or down stairs
												value matches {
													0|[local::at0024],
													1|[local::at0025],
													2|[local::at0026],
													3|[local::at0027],
													4|[local::at0028]
												}
											}
											ELEMENT[at0029] occurrences matches {0..1} matches {    -- 4. Sitting or lying
												value matches {
													0|[local::at0030],
													1|[local::at0031],
													2|[local::at0032],
													3|[local::at0033],
													4|[local::at0034]
												}
											}
											ELEMENT[at0042] occurrences matches {0..1} matches {    -- Pain score raw
												value matches {
													DV_COUNT matches {
														magnitude matches {|0..16|}
													}
												}
											}
										}
									}
									CLUSTER[at0047] occurrences matches {0..1} matches {    -- Function, daily living
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0048] occurrences matches {0..1} matches {    -- 5. Rising from sitting
												value matches {
													0|[local::at0049],
													1|[local::at0050],
													2|[local::at0051],
													3|[local::at0052],
													4|[local::at0053]
												}
											}
											ELEMENT[at0054] occurrences matches {0..1} matches {    -- 6. Standing
												value matches {
													0|[local::at0055],
													1|[local::at0056],
													2|[local::at0057],
													3|[local::at0058],
													4|[local::at0059]
												}
											}
											ELEMENT[at0060] occurrences matches {0..1} matches {    -- 7. Getting in/out of a car
												value matches {
													0|[local::at0061],
													1|[local::at0062],
													2|[local::at0063],
													3|[local::at0064],
													4|[local::at0065]
												}
											}
											ELEMENT[at0066] occurrences matches {0..1} matches {    -- 8. Twisting/pivoting on your injured knee
												value matches {
													0|[local::at0067],
													1|[local::at0068],
													2|[local::at0069],
													3|[local::at0070],
													4|[local::at0071]
												}
											}
											ELEMENT[at0072] occurrences matches {0..1} matches {    -- Function score raw
												value matches {
													DV_COUNT matches {
														magnitude matches {|0..16|}
													}
												}
											}
										}
									}
									CLUSTER[at0075] occurrences matches {0..1} matches {    -- Quality of Life
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0076] occurrences matches {0..1} matches {    -- 9. Awareness of knee problem
												value matches {
													0|[local::at0077],
													1|[local::at0078],
													2|[local::at0079],
													3|[local::at0080],
													4|[local::at0081]
												}
											}
											ELEMENT[at0131] occurrences matches {0..1} matches {    -- 10. Lifestyle modification to avoid knee damage
												value matches {
													0|[local::at0132],
													1|[local::at0133],
													2|[local::at0134],
													3|[local::at0135],
													4|[local::at0136]
												}
											}
											ELEMENT[at0137] occurrences matches {0..1} matches {    -- 11. Lack of confidence in knee
												value matches {
													0|[local::at0138],
													1|[local::at0139],
													2|[local::at0140],
													3|[local::at0141],
													4|[local::at0142]
												}
											}
											ELEMENT[at0143] occurrences matches {0..1} matches {    -- 12. Overall knee-related difficulty
												value matches {
													0|[local::at0144],
													1|[local::at0145],
													2|[local::at0146],
													3|[local::at0147],
													4|[local::at0148]
												}
											}
											ELEMENT[at0149] occurrences matches {0..1} matches {    -- Quality of life score raw
												value matches {
													DV_COUNT matches {
														magnitude matches {|0..16|}
													}
												}
											}
										}
									}
									CLUSTER[at0155] occurrences matches {0..1} matches {    -- Scoring transformed
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0151] occurrences matches {0..1} matches {    -- Pain score transformed
												value matches {
													DV_COUNT matches {
														magnitude matches {|0..100|}
													}
												}
											}
											ELEMENT[at0073] occurrences matches {0..1} matches {    -- Function score transformed
												value matches {
													DV_COUNT matches {
														magnitude matches {|0..100|}
													}
												}
											}
											ELEMENT[at0150] occurrences matches {0..1} matches {    -- Quality of life  score transformed
												value matches {
													DV_COUNT matches {
														magnitude matches {|0..100|}
													}
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0046] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0159] occurrences matches {0..1} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Knee Injury and Osteoarthritis Outcome Score - Short Form (KOOS-12)">
					description = <"Patient-reported outcome measure (PROM) capturing the 12-item KOOS-12 across three domains (Pain, Function, Quality of Life). Raw item responses (0–4) are stored; domain scores are computed by summated ratings and linearly transformed to 0–100 (higher = better). The Summary knee impact score (0–100) is the unweighted mean of the three domain scores and is not calculated if any domain score is missing; each domain requires ≥2/4 items answered with imputation for missing items.">
