	details = <
		["fr-ch"] = <
			language = <[ISO_639-1::fr-ch]>
			purpose = <"Archétype avec questions supplémentaires issues du SIRIS PROM pour le registre des implants de hanche, genou et épaule SIRIS.">
			copyright = <"© openEHR Foundation">
			use = <"Utilisez-le pour compléter le PROM EQ-5D-5L avec les trois questions supplémentaires destinées aux patients issues de l'enquête SIRIS pour le registre SIRIS des implants de hanche, de genou et d'épaule.">
			misuse = <"Ne l'utilisez pas pour documenter des résultats cliniques ou des faits. Il sert exclusivement de complément au questionnaire SIRIS destiné aux patients.">
		>
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"Archetype with additional questions from the SIRIS PROM for the SIRIS hip, knee and shoulder implant registry.">
			keywords = <"SIRIS, PROM, Hip, Knee, Shoulder", ...>
			use = <"Use it to supplement the PROM EQ-5D-5L with the three additional patient questions from the SIRIS patient survey for the SIRIS hip, knee and shoulder implant registry.">
			misuse = <"Do not use it to document clinical findings or facts. It is used exclusively as a supplement to the SIRIS patient questionnaire.">
		>
