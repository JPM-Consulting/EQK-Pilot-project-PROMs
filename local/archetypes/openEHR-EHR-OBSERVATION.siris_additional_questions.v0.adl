definition
	OBSERVATION[at0000] matches {    -- SIRIS additional questions
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									CLUSTER[at0004] occurrences matches {0..1} matches {    -- Joint specific pain
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0005] occurrences matches {0..1} matches {    -- Joint specific pain
												value matches {
													0|[local::at0006],
													1|[local::at0007],
													2|[local::at0008],
													3|[local::at0009],
													4|[local::at0010],
													5|[local::at0011],
													6|[local::at0012],
													7|[local::at0013],
													8|[local::at0014],
													9|[local::at0015],
													10|[local::at0016]
												}
											}
										}
									}
									CLUSTER[at0017] occurrences matches {0..1} matches {    -- Joint specific satisfaction
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0018] occurrences matches {0..1} matches {    -- Joint specific satisfaction
												value matches {
													0|[local::at0019],
													2|[local::at0020],
													3|[local::at0021],
													4|[local::at0022],
													5|[local::at0023]
												}
											}
										}
									}
									CLUSTER[at0024] occurrences matches {0..1} matches {    -- Your level of education
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0025] occurrences matches {0..1} matches {    -- Your level of education
												value matches {
													1|[local::at0026],
													2|[local::at0027],
													3|[local::at0028],
													4|[local::at0029]
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0030] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0031] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

