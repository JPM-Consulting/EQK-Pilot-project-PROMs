		["en-us"] = <
			language = <[ISO_639-1::en-us]>
			purpose = <"Evaluation archetype to record the value weights of individual responses and the derived index score from the EQ-5D-5L health status instrument.">
			keywords = <"EQ-5D-5L, Index Score", ...>
			copyright = <"© openEHR Foundation">
			use = <"Use this Evaluation archetype only to record the value weights of each response and the derived index score, in conjunction with the Composition archetype EQ-5D-5L Health Status.">
			misuse = <"*Do not use this Evaluation archetype as a questionnaire to capture the individual’s responses. Use the Composition archetype EQ-5D-5L Health Status for that purpose. (en)">
