definition
	EVALUATION[at0000] matches {    -- EQ 5D 5L Index Score
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					CLUSTER[at0002] occurrences matches {0..1} matches {    -- Dimensions
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0006] occurrences matches {0..1} matches {    -- Mobility weighted
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::380]>
										list = <
											["1"] = <
												units = <"1">
												magnitude = <|0.0..1.0|>
												precision = <|3|>
											>
										>
									>
								}
							}
							ELEMENT[at0007] occurrences matches {0..1} matches {    -- Self-care weighted
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::380]>
										list = <
											["1"] = <
												units = <"1">
												magnitude = <|0.0..1.0|>
												precision = <|3|>
											>
										>
									>
								}
							}
							ELEMENT[at0008] occurrences matches {0..1} matches {    -- Usual activities weighted
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::380]>
										list = <
											["1"] = <
												units = <"1">
												magnitude = <|0.0..1.0|>
												precision = <|3|>
											>
										>
									>
								}
							}
							ELEMENT[at0009] occurrences matches {0..1} matches {    -- Pain/discomfort weighted
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::380]>
										list = <
											["1"] = <
												units = <"1">
												magnitude = <|0.0..1.0|>
												precision = <|3|>
											>
										>
									>
								}
							}
							ELEMENT[at0010] occurrences matches {0..1} matches {    -- Anxiety/depression weighted
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::380]>
										list = <
											["1"] = <
												units = <"1">
												magnitude = <|0.0..1.0|>
												precision = <|3|>
											>
										>
									>
								}
							}
						}
					}
					CLUSTER[at0011] occurrences matches {0..1} matches {    -- Single index score
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0012] occurrences matches {0..1} matches {    -- Index Score
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::380]>
										list = <
											["1"] = <
												units = <"1">
												magnitude = <|-0.594..1.0|>
												precision = <|3|>
											>
										>
									>
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0013] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0014] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"EQ 5D 5L Index Score">
					description = <"To calculate an EQ-5D-5L score, first identify the 5-digit health state bycombining the individual's responses for each of the five dimensions (Mobility, Self-care, Usual Activities, Pain/Discomfort, Anxiety/Depression), with eachdigit representing a level from 1 (no problems) to 5 (extreme problems). Then, subtract the sum of the value weights for each response from 1, using a country-specific value set that assigns a specific weight to each health state. The result is a single index score between -0.594 and 1, where 1 indicates full health and values below 0 represent states worse than death.">
				>
				["at0001"] = <
					text = <"Item tree">
					description = <"@ internal @">
